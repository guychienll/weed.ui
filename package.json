{
  "name": "weed.ui",
  "version": "1.2.1",
  "description": "addictive react component library",
  "main": "build",
  "files": [
    "build",
    "components",
    "util"
  ],
  "author": "guychienll",
  "license": "MIT",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/guychienll/weed.ui.git"
  },
  "scripts": {
    "start": "npm run clean && mkdir -p build && cp package.build.json ./build/package.json && tsc -w",
    "build": "npm run clean && tsc && cp -r build/* ./",
    "clean": "rm -rf build components util *.d.ts *.js *.js.map",
    "eslint": "eslint --init",
    "release": "npm run build && dotenv release-it --"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.3.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "release-it": "^14.14.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3"
  }
}
